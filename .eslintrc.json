{
  "env": {
    "browser": true,
    "es2021": true,
    "commonjs": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "globals": {
    "getDomElement": true,
    "prevNextSlide": true,
    "closeLightBox": true,
    "LIGHTBOX_CONTROLS": true,
    "mediaFactory": true,
    "openLightBox": true,
    "photographerFactory": true,
    "displayModal": true,
    "setEventListener": true
  },
  "rules": {
    "no-unused-vars": 0
  }
}
